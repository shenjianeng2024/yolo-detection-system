{
  "productName": "YOLO Detection System",
  "version": "0.1.0",
  "identifier": "com.yolo-detection.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "YOLOv8实时检测系统",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "default",
          "description": "Default permissions for the app",
          "windows": ["main"],
          "permissions": [
            "dialog:allow-open",
            "dialog:default",
            "fs:allow-read-file",
            "fs:allow-read-text-file", 
            "fs:allow-exists",
            "fs:default",
            "core:path:default"
          ]
        }
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  },
  "plugins": {}
}